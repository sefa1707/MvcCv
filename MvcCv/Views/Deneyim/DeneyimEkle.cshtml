
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Yeni Deneyim Ekleme Sayfası</h2>

<form method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı yazınız" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Açıklama yazınız" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma Tarihi yazınız" />
    <br />
    <input type="text" name="Aciklama" class="form-control" placeholder="Detaylı Açıklama yazınız" />
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>
