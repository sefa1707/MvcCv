@model MvcCv.Models.Entity.TblSosyalMedya
@{
    ViewBag.Title = "SosyalMedyaGetir";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


<br />
<h2>Hesap Düzenleme Sayfası</h2>
<br />
<form method="post">
    @Html.Label("Sosyal Medya Adı")
    <br />
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya Linki")
    <br />
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya İkonu")
    <br />
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>
